entMenu--;\r\n    if(currentMenu<0){\r\n        currentMenu=menuItems.length-1;\r\n    }\r\n    allMenu(currentMenu);\r\n  })\r\n\r\n    //next \r\n    next.addEventListener('click',() =>{\r\n      //img++;\r\n    currentMenu++;\r\n      if(currentMenu>menuItems.length-1){\r\n        currentMenu=0;\r\n      }\r\n    allMenu(currentMenu);\r\n  })\r\n   "},"lineCount":null}},"error":null,"hash":"3773eef4d171a13813d69adaff7e61c1","cacheData":{"env":{}}}